spriteWidth($sprite)
  width $sprite[4];

spriteHeight($sprite)
  height $sprite[5];

spritePosition($sprite)
  background-position $sprite[2] $sprite[3];

spriteImage($sprite)
  background-image url($sprite[8]);

sprite($sprite)
  display block
  spriteImage($sprite)
  spritePosition($sprite)
  spriteWidth($sprite)
  spriteHeight($sprite)

getSprite($name)
  if $name is not ''
    $x = 's-company_' + $name
    $x = unquote($x)
    sprite($+$x)
     